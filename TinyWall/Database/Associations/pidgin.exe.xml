<?xml version="1.0" encoding="utf-8"?>
<Application Name="Pidgin">
	<Files>
    <AppExceptionAssoc Executable="pidgin.exe">
      <ExceptionTemplate Template="true">
        <OpenPortOutboundRemoteTCP>*</OpenPortOutboundRemoteTCP>
        <OpenPortListenLocalTCP>*</OpenPortListenLocalTCP>
        <OpenPortOutboundRemoteUDP>*</OpenPortOutboundRemoteUDP>
        <OpenPortListenLocalUDP>*</OpenPortListenLocalUDP>
      </ExceptionTemplate>
      <PublicKeys>
        <string></string>
      </PublicKeys>
      <SearchPaths>
        <string>{reg:SOFTWARE\pidgin:}</string>
        <string>{reg:SOFTWARE\Wow6432Node\pidgin:}</string>
      </SearchPaths>
    </AppExceptionAssoc>
	</Files>
</Application>
