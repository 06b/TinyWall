<?xml version="1.0" encoding="utf-8"?>
<Profile Name="Web browser">
  <Rules>
    <RuleDef Name="HTTP(S) client">
      <Action>Allow</Action>
      <Protocol>TCP</Protocol>
      <Direction>Out</Direction>
      <RemotePorts>80,443</RemotePorts>
    </RuleDef>
    <RuleDef Name="RTSP client (control)">
      <Action>Allow</Action>
      <Protocol>TcpUdp</Protocol>
      <Direction>Out</Direction>
      <RemotePorts>554</RemotePorts>
    </RuleDef>
    <RuleDef Name="Browser out">
      <Action>Allow</Action>
      <Protocol>TCP</Protocol>
      <Direction>Out</Direction>
      <RemotePorts>EPHEMERAL</RemotePorts>
    </RuleDef>
  </Rules>
</Profile>