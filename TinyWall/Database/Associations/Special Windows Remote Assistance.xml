<?xml version="1.0" encoding="utf-8"?>
<Application Name="Windows Remote Assistance" Special="true">
	<Files>
	<AppExceptionAssoc Executable="{folder:sys32}\svchost.exe" Service="PNRPsvc">
      <ExceptionTemplate Template="true">
        <Profiles>
          <string>PNRP</string>
        </Profiles>
      </ExceptionTemplate>
	</AppExceptionAssoc>
	<AppExceptionAssoc Executable="{folder:sys32}\svchost.exe" Service="ssdpsrv">
      <ExceptionTemplate Template="true">
        <Profiles>
          <string>SSDP</string>
        </Profiles>
      </ExceptionTemplate>
	</AppExceptionAssoc>
	<AppExceptionAssoc Executable="{folder:sys32}\raserver.exe">
      <ExceptionTemplate Template="true">
        <OpenPortOutboundRemoteTCP>*</OpenPortOutboundRemoteTCP>
        <OpenPortListenLocalTCP>*</OpenPortListenLocalTCP>
        <OpenPortOutboundRemoteUDP>*</OpenPortOutboundRemoteUDP>
        <OpenPortListenLocalUDP>*</OpenPortListenLocalUDP>
      </ExceptionTemplate>
	</AppExceptionAssoc>
	<AppExceptionAssoc Executable="{folder:sys32}\msra.exe">
      <ExceptionTemplate Template="true">
        <OpenPortOutboundRemoteTCP>*</OpenPortOutboundRemoteTCP>
        <OpenPortListenLocalTCP>*</OpenPortListenLocalTCP>
        <OpenPortOutboundRemoteUDP>*</OpenPortOutboundRemoteUDP>
        <OpenPortListenLocalUDP>*</OpenPortListenLocalUDP>
      </ExceptionTemplate>
	</AppExceptionAssoc>  
	</Files>
</Application>