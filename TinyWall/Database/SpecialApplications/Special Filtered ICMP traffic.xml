<?xml version="1.0" encoding="utf-8"?>
<Application xmlns:i="http://www.w3.org/2001/XMLSchema-instance" xmlns="TinyWall">
  <Components>
    <SubjectIdentity>
      <Policy i:type="RuleListPolicy">
        <Rules>
          <RuleDef>
            <Action>Allow</Action>
            <Direction>Out</Direction>
            <IcmpTypesAndCodes>8:0</IcmpTypesAndCodes>
            <Name>ICMPv4 (echo-req) out</Name>
            <Protocol>ICMPv4</Protocol>
          </RuleDef>
          <RuleDef>
            <Action>Allow</Action>
            <Direction>Out</Direction>
            <IcmpTypesAndCodes>128:0</IcmpTypesAndCodes>
            <Name>ICMPv6 (echo-req) out</Name>
            <Protocol>ICMPv6</Protocol>
          </RuleDef>
          <RuleDef>
            <Action>Allow</Action>
            <Direction>In</Direction>
            <RemoteAddresses>Defaultgateway</RemoteAddresses>
            <IcmpTypesAndCodes>3:*,11:*</IcmpTypesAndCodes>
            <Name>ICMPv4 (safe)</Name>
            <Protocol>ICMPv4</Protocol>
          </RuleDef>
          <RuleDef>
            <Action>Allow</Action>
            <Direction>In</Direction>
            <IcmpTypesAndCodes>129:0,1:*,3:*,2:0</IcmpTypesAndCodes>
            <Name>ICMPv6 (safe)</Name>
            <Protocol>ICMPv6</Protocol>
          </RuleDef>
        </Rules>
      </Policy>
      <Subject i:type="GlobalSubject" />
    </SubjectIdentity>
  </Components>
  <Flags xmlns:d2p1="http://schemas.microsoft.com/2003/10/Serialization/Arrays">
    <d2p1:KeyValueOfstringstring>
      <d2p1:Key>TWUI:SPECIAL</d2p1:Key>
      <d2p1:Value i:nil="true" />
    </d2p1:KeyValueOfstringstring>
    <d2p1:KeyValueOfstringstring>
      <d2p1:Key>TWUI:RECOMMENDED</d2p1:Key>
      <d2p1:Value i:nil="true" />
    </d2p1:KeyValueOfstringstring>
  </Flags>
  <Name>Filtered_ICMP_Traffic</Name>
</Application>