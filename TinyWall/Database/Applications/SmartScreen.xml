<?xml version="1.0" encoding="utf-8"?>
<Application xmlns:i="http://www.w3.org/2001/XMLSchema-instance" xmlns="TinyWall">
  <Components>
    <SubjectIdentity>
      <Policy i:type="TcpUdpPolicy">
        <AllowedRemoteTcpConnectPorts>*</AllowedRemoteTcpConnectPorts>
      </Policy>
      <Subject i:type="ExecutableSubject">
        <ExecutablePath>{folder:sys32}\smartscreen.exe</ExecutablePath>
      </Subject>
    </SubjectIdentity>
  </Components>
  <Name>Windows SmartScreen</Name>
</Application>