<?xml version="1.0" encoding="utf-8"?>
<Application Name="Windows_Update" Recommended="true" Special="true">
	<Files>
	<AppExceptionAssoc Executable="{folder:sys32}\svchost.exe" Service="wuauserv">
      <ExceptionTemplate Template="true">
        <OpenPortOutboundRemoteTCP>*</OpenPortOutboundRemoteTCP>
      </ExceptionTemplate>
	</AppExceptionAssoc>
	<AppExceptionAssoc Executable="{folder:sys32}\svchost.exe">
      <ExceptionTemplate Template="true">
        <OpenPortOutboundRemoteTCP>443,80</OpenPortOutboundRemoteTCP>
      </ExceptionTemplate>
	</AppExceptionAssoc>
	</Files>
</Application>