<?xml version="1.0" encoding="utf-8"?>
<Application Name="Windows_DHCP_Client" Recommended="true" Special="true">
	<Files>
	<AppExceptionAssoc Executable="{folder:sys32}\svchost.exe" Service="dhcp">
      <ExceptionTemplate Template="true">
        <Profiles>
          <string>DHCP client</string>
        </Profiles>
      </ExceptionTemplate>
	</AppExceptionAssoc>
	<AppExceptionAssoc Executable="{folder:sys32}\svchost.exe" Service="lmhosts">
      <ExceptionTemplate Template="true">
        <OpenPortOutboundRemoteUDP>67</OpenPortOutboundRemoteUDP>
      </ExceptionTemplate>
	</AppExceptionAssoc>
	</Files>
</Application>
