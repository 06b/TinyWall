<?xml version="1.0" encoding="utf-8"?>
<Application Name="Microsoft Security Essentials" Recommended="true">
	<Files>
    <AppExceptionAssoc Executable="msseces.exe">
      <ExceptionTemplate Template="true">
        <OpenPortOutboundRemoteTCP>*</OpenPortOutboundRemoteTCP>
      </ExceptionTemplate>
      <PublicKeys>
        <string></string>
      </PublicKeys>
      <SearchPaths>
        <string>{reg:SOFTWARE\Microsoft\Microsoft Antimalware:InstallLocation}</string>
        <string>{reg:SOFTWARE\Wow6432Node\Microsoft\Microsoft Antimalware:InstallLocation}</string>
      </SearchPaths>
    </AppExceptionAssoc>
    <AppExceptionAssoc Executable="NisSrv.exe" Service="NisSrv">
      <ExceptionTemplate Template="true">
        <OpenPortOutboundRemoteTCP>*</OpenPortOutboundRemoteTCP>
        <OpenPortListenLocalTCP>*</OpenPortListenLocalTCP>
        <OpenPortOutboundRemoteUDP>*</OpenPortOutboundRemoteUDP>
        <OpenPortListenLocalUDP>*</OpenPortListenLocalUDP>
      </ExceptionTemplate>
      <PublicKeys>
        <string></string>
      </PublicKeys>
      <SearchPaths>
        <string>{reg:SOFTWARE\Microsoft\Microsoft Antimalware:InstallLocation}</string>
        <string>{reg:SOFTWARE\Wow6432Node\Microsoft\Microsoft Antimalware:InstallLocation}</string>
      </SearchPaths>
    </AppExceptionAssoc>
	</Files>
</Application>
