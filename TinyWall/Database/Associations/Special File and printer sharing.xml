<?xml version="1.0" encoding="utf-8"?>
<Application Name="File_and_Printer_Sharing" Recommended="false" Special="true">
	<Files>
    <AppExceptionAssoc Executable="System">
      <ExceptionTemplate Template="true">
        <Profiles>
          <string>NetBIOS</string>
        </Profiles>
      </ExceptionTemplate>
    </AppExceptionAssoc>
	<AppExceptionAssoc Executable="{folder:sys32}\spoolsv.exe">
      <ExceptionTemplate Template="true">
        <LocalNetworkOnly>true</LocalNetworkOnly>
        <OpenPortOutboundRemoteTCP>*</OpenPortOutboundRemoteTCP>
        <OpenPortListenLocalTCP>*</OpenPortListenLocalTCP>
        <OpenPortOutboundRemoteUDP>*</OpenPortOutboundRemoteUDP>
        <OpenPortListenLocalUDP>*</OpenPortListenLocalUDP>
      </ExceptionTemplate>
	</AppExceptionAssoc>
	</Files>
</Application>