<?xml version="1.0" encoding="utf-8"?>
<Application xmlns:i="http://www.w3.org/2001/XMLSchema-instance" xmlns="TinyWall">
  <Components>
    <SubjectIdentity>
      <CertificateSubjects xmlns:d4p1="http://schemas.microsoft.com/2003/10/Serialization/Arrays">
        <d4p1:string>CN=Google LLC, O=Google LLC, L=Mountain View, S=ca, C=US</d4p1:string>
      </CertificateSubjects>
      <Policy i:type="TcpUdpPolicy">
        <AllowedLocalTcpListenerPorts>*</AllowedLocalTcpListenerPorts>
        <AllowedLocalUdpListenerPorts>*</AllowedLocalUdpListenerPorts>
        <AllowedRemoteTcpConnectPorts>*</AllowedRemoteTcpConnectPorts>
        <AllowedRemoteUdpConnectPorts>*</AllowedRemoteUdpConnectPorts>
      </Policy>
      <SearchPaths xmlns:d4p1="http://schemas.microsoft.com/2003/10/Serialization/Arrays">
        <d4p1:string>{folder:LocalAppData}\Google\Chrome\Application</d4p1:string>
        <d4p1:string>{folder:pf32}\Google\Chrome\Application</d4p1:string>
        <d4p1:string>{folder:pf64}\Google\Chrome\Application</d4p1:string>
      </SearchPaths>
      <Subject i:type="ExecutableSubject">
        <ExecutablePath>chrome.exe</ExecutablePath>
      </Subject>
    </SubjectIdentity>
  </Components>
  <Name>Google Chrome</Name>
</Application>