<?xml version="1.0" encoding="utf-8"?>
<Application Name="Windows_Remote_Desktop" Special="true">
	<Files>
	<AppExceptionAssoc Executable="System">
      <ExceptionTemplate Template="true">
        <Profiles>
          <string>RDP Server</string>
        </Profiles>
      </ExceptionTemplate>
	</AppExceptionAssoc>
	<AppExceptionAssoc Executable="{folder:sys32}\svchost.exe">
      <ExceptionTemplate Template="true">
        <Profiles>
          <string>RDP Server</string>
        </Profiles>
      </ExceptionTemplate>
	</AppExceptionAssoc>
	</Files>
</Application>