<?xml version="1.0" encoding="utf-8"?>
<Application Name="Windows Time Sync" Recommended="true" Special="true">
	<Files>
	<AppExceptionAssoc Executable="{folder:sys32}\svchost.exe" Service="W32Time">
      <ExceptionTemplate Template="true">
        <Profiles>
          <string>Time synchronization</string>
        </Profiles>
      </ExceptionTemplate>
	</AppExceptionAssoc>
	</Files>
</Application>
